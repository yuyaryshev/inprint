{
    "name": "inprint",
    "version": "1.2.7",
    "private": false,
    "description": "A very simple precompiler written in typescript. With it you can make a dynamically generated blocks in your code.",
    "keywords": [
        "generator",
        "precompiler",
        "inprint",
        "inprinter"
    ],
    "main": "./lib/index.js",
    "bin": {
        "inprint": "./bin/index.js"
    },
    "files": [
        ".gitignore",
        "package.json",
        "LICENSE",
        "bin/**/*.js",
        "lib/**/*.js",
        "readme.md"
    ],
    "scripts": {
        "test": "jest",
        "watch": "babel src --config-file ./babel.ts.config.js --out-dir lib --extensions \".ts,.tsx\" --source-maps inline -w",
        "build": "babel src --config-file ./babel.ts.config.js --out-dir lib --extensions \".ts,.tsx\" --source-maps inline",
        "ycplmon": "node ..\\ycplmon_amaglion\\ts_out\\src\\ycplmon.js src",
        "tsc": "tsc",
        "start": "node bin/index.js"
    },
    "author": "Yuri Yaryshev",
    "license": "Unlicense",
    "homepage": "https://github.com/yuyaryshev/inprint",
    "repository": {
        "type": "git",
        "url": "https://github.com/yuyaryshev/inprint"
    },
    "plainDependencies": true,
    "jest": {
        "rootDir": "lib"
    },
    "prettier": {
        "tabWidth": 4,
        "printWidth": 120,
        "trailingComma": "es5"
    },
    "husky": {
        "hooks": {
            "pre-commit": "pretty-quick --staged"
        }
    },
